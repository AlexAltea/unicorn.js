      glib_size_type=short
      ;;
  $ac_cv_sizeof_int)
      glib_size_type=int
      ;;
  $ac_cv_sizeof_long)
      glib_size_type=long
      ;;
  *)  { { echo "$as_me:$LINENO: error: No type matching size_t in size" >&5
echo "$as_me: error: No type matching size_t in size" >&2;}
   { (exit 1); exit 1; }; }
      ;;
esac

if test $ac_cv_sizeof_size_t = $ac_cv_sizeof_int &&
   test $ac_cv_sizeof_size_t = $ac_cv_sizeof_long ; then
  cat >conftest.$ac_ext <<_ACEOF

#if defined(_AIX) && !defined(__GNUC__)
#pragma options langlvl=stdc89
#endif
#include <stddef.h>
int main ()
{
  size_t s = 1;
  unsigned int *size_int = &s;
  return (int)*size_int;
}

